{
  "env": {
      "es2020": true,
      "node": true,
      "jasmine": true
  },
  "extends": [
      "standard", "prettier", "plugin:jasmine/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint", "prettier", "jasmine"

  ],
  "rules": {
   "prettier/prettier": "error"
  }
}